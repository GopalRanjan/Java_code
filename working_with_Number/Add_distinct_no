import java.util.*;

public class SumOf_DistinctNo
{
    public static void main(String[] args)
    {
        int[] a = {1,4,5,7,8,9,6,4,5,1};
        Set s = new HashSet();
        int sum =0;
        for(int i=0;i<a.length;i++)
        {
            if(s.add(a[i]))
            {
                sum += a[i];
            }
        }
        System.out.println("Sum of Distinct No: " +sum);
    }
}