import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    String s = "abcabcdefab";
	    Set s1 = new HashSet();
	    int l=0,m=0,si=0,r=0;
	    for(r=0;r<s.length();r++)
	    {
	        while(s1.contains(s.charAt(r)))
	        {
	            s1.remove(s.charAt(l));
	            l++;
	        }
	        s1.add(s.charAt(r));
	        if(r-l+1>m)
	        {
	            m = r-l+1;
	            si=l;
	        }
	    }
		System.out.println(s.substring(si,m+si));
	}
}
